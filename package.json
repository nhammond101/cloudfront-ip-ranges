{
  "name": "cloudfront-ip-ranges",
  "version": "2.0.0",
  "description": "Get Cloudfront IPs for use in trust proxy configurations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc -d",
    "build:watch": "npm run build -- -w",
    "clean:all": "npm run clean && npm run clean:modules && npm run clean:tests && npm clean:coverage",
    "clean:coverage": "rimraf coverage",
    "clean:modules": "rimraf node_modules && rimraf dist/node_modules",
    "clean:tests": "rimraf test-reports",
    "clean": "rimraf dist",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\" --ignore-path ./.prettierignore",
    "lint": "eslint -c .eslintrc.js .",
    "prepare": "npm run build",
    "pretest": "jest --clearCache",
    "test": "jest --coverage",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nhammond101/cloudfront-ip-ranges.git"
  },
  "keywords": [
    "cloudfront",
    "express",
    "trust",
    "proxy",
    "trust proxy",
    "cloudfront ip",
    "cloudfront ips"
  ],
  "author": "Nick Hammond",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhammond101/cloudfront-ip-ranges/issues"
  },
  "homepage": "https://github.com/nhammond101/cloudfront-ip-ranges#readme",
  "devDependencies": {
    "@types/debug": "4.1.7",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.19",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "eslint": "7.32.0",
    "eslint-config-waracle": "2.0.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-jest": "24.4.2",
    "husky": "4.3.8",
    "jest": "27.2.2",
    "jest-each": "27.2.2",
    "jest-junit": "12.3.0",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "semantic-release": "18.0.0",
    "ts-jest": "27.0.5",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">10"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "debug": "4.3.2"
  },
  "optionalDependencies": {
    "@types/express": "4.17.13",
    "express": "4.17.1"
  }
}
